<div>
    <h1>list cars page</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">_id</th>
                <th scope="col">ubezpieczony</th>
                <th scope="col">benzyna</th>
                <th scope="col">uszkodzony</th>
                <th scope="col">naped4x4</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            {{#each Doc}}
            <tr>
                <td>{{this._id}}</td>
                <td>{{this.ubezpieczony}}</td>
                <td>{{this.benzyna}}</td>
                <td>{{this.uszkodzony}}</td>
                <td>{{this.naped}}</td>
                <td>
                    <button type="submit" class="btn btn-dark"
                        onclick="alert(`id: {{this._id}} \nubezpieczony: {{this.ubezpieczony}} \nbenzyna: {{this.benzyna}} \nuszkodzony: {{this.uszkodzony}} \nnaped: {{this.naped}}`)">
                        INFO
                    </button>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>